{
  "name": "@dicitech/amo-kommo-types",
  "version": "1.0.0",
  "description": "Unofficial TypeScript type definitions for AmoCRM and Kommo API + Widgets. Comprehensive type safety for REST API entities, widget development, and CRM integration.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "build:check": "tsc --noEmit",
