    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DiciTechcom/amo-kommo-types.git"
  },
  "keywords": [
    "amocrm",
    "amocrm-api",
    "amocrm-v4",
    "kommo",
    "kommo-widget",
    "typescript",
    "types",
    "type-definitions",
    "crm",
    "api",
    "interfaces",
    "widget"
  ],
  "author": "Dicitech <info@dicitech.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DiciTechcom/amo-kommo-types/issues"
  },
  "homepage": "https://github.com/DiciTechcom/amo-kommo-types#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "rimraf": "~6.0.1",
    "typescript": "~3.9.10"
  }
